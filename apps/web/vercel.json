{
  "crons": [
    {
      "path": "/api/cron/cleanup-auth",
      "schedule": "0 3 * * *"
    },
    {
      "path": "/api/cron/decay-ranking-scores",
      "schedule": "0 0 * * 0"
    },
    {
      "path": "/api/cron/process-ranking-events",
      "schedule": "0 0 * * *"
    }
  ],
  "headers": [
    {
      "source": "/favicon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, immutable, max-age=31536000"
        }
      ]
    },
    {
      "source": "/logos/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, immutable, max-age=31536000"
        }
      ]
    }
  ]
}
